{
  "name": "dashboard-boilerplate",
  "version": "2.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.4.0",
    "@craco/craco": "^6.1.0",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@umijs/hooks": "^1.9.3",
    "antd": "^4.12.0",
    "axios": "^0.21.1",
    "craco-less": "^1.17.1",
    "dayjs": "^1.10.4",
    "formik": "^2.2.6",
    "js-cookie": "^2.2.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "rc-picker": "^2.5.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-infinite-scroller": "^1.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-toastify": "^7.0.3",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco --max_old_space_size=4096 build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "node_modules/.bin/eslint src/*.jsx && node_modules/.bin/eslint src/views/**/*.jsx && node_modules/.bin/eslint src/components/**/*.jsx",
    "code-check": "prettier --check . && yarn lint",
    "code-format": "prettier --write . && yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn code-check",
      "pre-push": "yarn code-format"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1"
  }
}
